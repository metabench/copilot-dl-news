{
  "name": "news_crawler",
  "version": "0.0.13",
  "description": "A web crawler for news articles with navigation detection",
  "main": "src/crawl.js",
  "scripts": {
    "start": "node src/crawl.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --forceExit --reporters=default --reporters=./jest-timing-reporter.js",
    "test:timing": "node --experimental-vm-modules node_modules/jest/bin/jest.js --forceExit --reporters=default --reporters=./jest-timing-reporter.js",
    "test:file": "node --experimental-vm-modules node_modules/jest/bin/jest.js --forceExit --reporters=default --reporters=./jest-timing-reporter.js --testPathPattern",
    "test:suite": "node tests/run-tests.js",
    "test:unit": "node tests/run-tests.js unit",
    "test:integration": "node tests/run-tests.js integration",
    "test:e2e": "node tests/run-tests.js e2e",
    "test:e2e-quick": "node tests/run-tests.js e2e-quick",
    "test:all": "node tests/run-tests.js all",
    "test:dev-geography": "node tests/run-tests.js dev-geography",
    "test:dev-geography-monitor": "node tests/run-tests.js dev-geography-monitor",
    "tests:review": "node tests/tests-review.js",
    "test:legacy:fast": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathIgnorePatterns='e2e|online|puppeteer|http.test.js' --maxWorkers=4",
    "test:legacy:quick": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern='quick.test.js' --forceExit --bail",
    "test:legacy:geography-full": "cross-env GEOGRAPHY_FULL_E2E=1 JEST_DISABLE_TRUNCATE=1 node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern='geography.full.e2e' --reporters=./jest-compact-reporter.js",
    "test:legacy:online": "ONLINE_TESTS=1 node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern='online'",
    "test:legacy:all": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles",
    "backfill:dates": "node src/tools/backfill-dates.js",
    "backfill:dates:redo": "node src/tools/backfill-dates.js --redo",
    "analyze:domains": "node src/tools/analyze-domains.js",
    "analyze:pages": "node src/analyse-pages.js",
    "analysis:run": "node src/tools/analysis-run.js",
    "populate:gazetteer": "node src/tools/populate-gazetteer.js --import-adm1=1 --import-cities=1 --cities-per-country=50 --include-blocs=ALL",
    "export:gazetteer": "node src/tools/export-gazetteer.js",
    "validate:gazetteer": "node src/tools/validate-gazetteer.js",
    "benchmarks": "node tools/benchmarks/run.js"
  },
  "keywords": [
    "crawler",
    "news",
    "scraping",
    "theguardian"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mozilla/readability": "^0.5.0",
    "better-sqlite3": "^12.2.0",
    "chalk": "^4.1.2",
    "cheerio": "^1.1.2",
    "compression": "^1.7.4",
    "express": "^5.0.0",
    "fast-xml-parser": "^4.3.6",
    "jsdom": "^24.0.0",
    "lang-tools": "^0.0.39",
    "ndjson": "^2.0.0",
    "node-fetch": "^3.3.2",
    "robots-parser": "^3.0.1",
    "tar-stream": "^3.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "babel-jest": "^29.7.0",
    "cross-env": "^10.1.0",
    "esbuild": "^0.24.2",
    "extract-zip": "^2.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "puppeteer": "^22.11.0",
    "sass": "^1.93.2",
    "sharp": "^0.34.4",
    "supertest": "^6.3.4"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/helpers/",
      "/helpers/",
      "database-helpers.js",
      "/tests/broken/",
      "/src/deprecated-ui/",
      "/tests/deprecated-ui/"
    ],
    "reporters": [
      "default",
      "<rootDir>/jest-reporter-truncate.js"
    ],
    "maxWorkers": 8,
    "testTimeout": 10000,
    "slowTestThreshold": 2,
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "watchman": false,
    "watch": false,
    "watchAll": false
  }
}
