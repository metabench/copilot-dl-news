{
  "name": "z-server",
  "version": "1.0.0",
  "description": "Industrial Luxury Obsidian Server Manager",
  "main": "main.js",
  "scripts": {
    "build": "esbuild renderer.src.js --bundle --outfile=renderer.js --platform=browser --format=iife --define:global=window",
    "start": "npm run build && electron .",
    "dev": "npm run build && electron .",
    "test": "jest --config jest.config.js",
    "test:unit": "jest --config jest.config.js tests/unit/",
    "test:smoke": "jest --config jest.config.js tests/smoke.test.js",
    "test:e2e": "npm run build && jest --config jest.e2e.config.js",
    "test:all": "npm run test:unit && npm run test:e2e"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^28.0.0",
    "esbuild": "^0.27.0",
    "jest": "^29.7.0",
    "playwright": "^1.40.0"
  },
  "dependencies": {
    "jsgui3-client": "^0.0.121",
    "ps-list": "^8.1.1",
    "tree-kill": "^1.2.2"
  }
}
